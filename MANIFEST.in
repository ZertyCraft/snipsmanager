exclude .gitignore
exclude .coverage
exclude .travis.yml
exclude Makefile
exclude Snipsfile
exclude test.py
include README.rst
include setup.cfg
include snipsskills/config/*
include snipsskills/data/sounds/*
prune .cache
prune .git
prune .snips
prune build
prune dist
recursive-exclude *.egg-info *
