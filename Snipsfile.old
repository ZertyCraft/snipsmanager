assistant: https://external-gateway.snips.ai/v1/registry/assistants/proj_9po3YAeBV/download?apiToken=BJWb0fIsClryMb0zLo0lrJmWCGLoCxrkVbRMIiAg
locale: en_US
logging: True
microphone:
  identifier: respeaker
  params:
    vendor_id: "2886"
    product_id: "0007"
leds: respeaker
default_location: Paris,fr
tts:
  service: snips
mqtt_broker:
  hostname: localhost
  port: 1883
skills:
  - package_name: snipshue
    pip: snipshue=0.1.6
    params:
      hostname: 192.168.163.96
      username: XyWT7TlG3RkMAzbKRt4fWpHEo1TudukIgH2OKz79
      light_ids: [1, 2, 3, 4, 5, 6]
    intents:
    - intent: DeactivateObject
      action: "turn_off"
    - intent: ActivateLightColor
      action: >
        {%
        if intent.objectColor != None:
          skill.set_color_name(intent.objectColor)
        else:
          skill.turn_on()
        %}
  - package_name: snipssmartercoffee
    class_name: SnipsSmarterCoffee
    pip: snipssmartercoffee
    params:
      hostname: 192.168.163.105
    intents:
    - intent: BrewCoffee
      action: "brew"
  - package_name: snipsfakeweather
    pip: snips-skill-fakeweather
    requires_tts: True
  - package_name: snipslocalmusic
    class_name: SnipsLocalMusic
    pip: snipslocalmusic
    params:
      db_file_path: data/music/db.json
